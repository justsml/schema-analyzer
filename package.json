{
  "name": "schema-analyzer",
  "version": "1.0.0",
  "description": "Heuristic JSON \"column\" type & size analysis w/ enumeration detection.",
  "main": "dist/schema-analyzer.cjs.js",
  "module": "dist/schema-analyzer.esm.js",
  "browser": "dist/schema-analyzer.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest --coverage",
    "test:debug": "node --inspect-brk=9823 ./node_modules/.bin/jest --coverage",
    "pretest": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justsml/schema-analyzer.git"
  },
  "keywords": [],
  "author": {
    "name": "Dan Levy",
    "url": "https://danlevy.net/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/justsml/schema-analyzer/issues"
  },
  "homepage": "https://github.com/justsml/schema-analyzer#readme",
  "dependencies": {
    "core-js": "^3.6.4",
    "lodash.camelcase": "^4.3.0",
    "lodash.isdate": "^4.0.1",
    "lodash.isnumber": "^3.0.3",
    "lodash.snakecase": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/runtime-corejs3": "^7.8.3",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@types/jest": "^25.1.0",
    "babel-jest": "^25.1.0",
    "csv-parse": "^4.8.5",
    "jest": "^25.1.0",
    "rollup": "^1.30.1",
    "rollup-plugin-node-globals": "^1.4.0",
    "typescript": "^3.7.5"
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": 3
        }
      ]
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": 3
        }
      ]
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "CI=true npm test"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
